long binary(long a,long b,long mod)
{
    long ans=1;
    a=a%mod;
    while(b)
    {
        if(b&1)
        {
          ans=(ans*a)%mod;
        }
        a=(a*a)%mod;
        b=(b>>1);
    }
    return ans;
}
long long int comb(long long int n,long long int r,long long int mod,vector<long long int>&fact)
{
    long long int a=fact[n];
    long long int b=binary(fact[r],mod-2,mod);
    long long int c=binary(fact[n-r],mod-2,mod);
    return (((a*b)%mod)*c)%mod;
}
